function init_state(state,p,t)
	-- player --
	if p != nil then
		p.state = state
		if p.state.init != nil then
			p.state.init(p)
		end
	end
	
	-- tot --
	if t != nil then
		t.state = state
		if t.state.t_init != nil then
			t.state.t_init(t)
		end
	end
end

