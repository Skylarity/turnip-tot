test_obj = {
	tile=0,--empty sprite
	update=function(this)
		this.x = this.x+sin(time())
		this.y = this.y+cos(time())
	end,
	draw=function(this)
	local col = 8
		rectfill(this.x,this.y,this.x+8,this.y+8,col)
	end
}
add(classes,test_obj)
init_obj(test_obj,60,60)

