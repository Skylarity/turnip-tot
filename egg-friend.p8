function _update()
	--todo
	
	-- screenshake --
	if shake>0 then
		shake-=1
		camera()
		if shake>0 then
			camera(-2+rnd(5),-2+rnd(5))
		end
	end
	
	-- update objects --
	foreach(objects,function(obj)
		if obj.class.update!=nil then
			obj.class.update(obj)
		end
	end)
end

function _draw()
	-- reset palette --
	pal()
	
	-- clear screen --
	cls()
	
	-- draw objects --
	foreach(objects,draw_object)
end

